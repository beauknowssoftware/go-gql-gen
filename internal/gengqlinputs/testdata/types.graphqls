type Other {
  name: Int
}

type MyType {
  id: ID
  myId: ID
  name: String
  names: [String]
  other: Other @resolve
  parent: Other
  others: [Other]
  pOthers(id: Int, name: String): OtherA @resolve
  iOthers(input: MyTypeInput): OtherB @resolve
}

input MyTypeInput {
   id: ID
   otherId: ID
   name: String
   count: Int
}

type Query {
    ping: [String]
}

type Mutation {
  save(id: ID): ID
}

schema {
    query: Query
    mutation: Mutation
}
